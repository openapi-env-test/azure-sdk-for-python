              "parameters": {
               "setting": {
                "type": "String",
                "metadata": {
                 "displayName": "Audit Setting"
                },
                "allowedValues": [
                 "enabled",
                 "disabled"
                ]
               }
              },
              "policyRule": {
               "if": {
                "field": "type",
                "equals": "Microsoft.Sql/servers/databases"
