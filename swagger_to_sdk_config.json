{
  "advancedOptions": {
    "createSdkPullRequests": true,
    "generationCallMode": "one-for-all-configs"
  },

  "initOptions": {
    "initScript": {
      "path": "chmod +x scripts/automation_init"
    }
  },

  "generateOptions": {
    "generateScript": {
      "path": "chmod +x scripts/automation_generate",
      "stderr": {
        "showInComment": true
      },
      "stdout": {
        "showInComment": "^\\[Autorest\\]"
      }
    },

    "parseGenerateOutput": true
  }
}